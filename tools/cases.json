{
  "series_spy": {
    "method": "POST",
    "path": "/expr/series",
    "payload": {
      "expr": "EQ:SPY",
      "duration": "30 D",
      "bar_size": "1 day",
      "use_rth": true
    },
    "expect": {
      "required_keys": ["expr", "label", "count", "points"],
      "require_non_empty_series": true
    },
    "tags": ["core", "expr"]
  },

  "chart_spy": {
    "method": "POST",
    "path": "/expr/chart",
    "payload": {
      "expr": "EQ:SPY",
      "duration": "30 D",
      "bar_size": "1 day",
      "use_rth": true
    },
    "expect": {
      "required_keys": ["expr", "series", "meta"],
      "require_non_empty_series": true
    },
    "tags": ["core", "expr"]
  },

  "pack_spy_full": {
    "method": "POST",
    "path": "/expr/pack",
    "payload": {
      "expr": "EQ:SPY",
      "want": ["price", "rsi", "bb", "drawdown", "sharpe", "stats"],
      "duration": "3 Y",
      "bar_size": "1 day",
      "use_rth": true
    },
    "expect": {
      "required_keys": ["expr", "series", "stats", "meta"],
      "require_non_empty_series": true
    },
    "tags": ["core", "pack"]
  },

  "corr_spy_qqq": {
    "method": "POST",
